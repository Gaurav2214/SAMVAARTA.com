        "scss-watch": "node-sass --watch resources/scss -o resources/css"
    },
    "devDependencies": {
        "axios": "^1.7.2",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14"
    },
    "dependencies": {
        "node-sass": "^9.0.0",
        "vite": "^5.3.3"
    }
}
